    "view": {
      "width": "500",
      "height": "300",
      "minWidth": "200",
      "minHeight": "100"
    },
    "apis": {
      "source": {
        "handler": "render",
        "description": "接口描述",
        "fields": {
          "value": {
            "description": "值说明"
          }
        }
      }
    },
    "config": {
      "xField": {
        "name": "x轴字段",
        "type": "string",
        "default": "xb"
      },
      "yField": {
        "name": "y轴字段",
        "type": "string",
        "default": "nld"
      },
      "valueField": {
        "name": "值字段",
        "type": "string",
        "default": "rs"
      },
      "color": {
        "name": "颜色",
        "type": "string",
        "default": "#60B0FF-#297AFC=#F29318-#FFD300"
