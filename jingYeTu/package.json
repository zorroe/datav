      "legend": {
        "name": "图例",
        "type": "group",
        "children": {
          "isShow": {
            "name": "是否显示",
            "type": "boolean"
          },
          "left": {
            "name": "距左",
            "type": "string"
          },
          "top": {
            "name": "距顶",
            "type": "string"
          },
          "itemGap": {
            "name": "图例间隔",
            "type": "number"
          },
          "itemWidth": {
            "name": "图形宽度",
            "type": "number"
          },
          "itemHeight": {
            "name": "图形高度",
            "type": "number"
          },
          "textColor": {
            "name": "文字颜色",
            "type": "string"
          },
          "fontSize": {
            "name": "文字大小",
            "type": "number"
          }
        },
        "default": {
          "isShow": true,
          "left": "center",
          "top": "10",
          "itemGap": 10,
          "itemWidth": 10,
          "itemHeight": 10,
          "textColor": "#333",
          "fontSize": 12
        }
      },
      "grid": {
        "name": "grid",
        "type": "group",
        "children": {
          "top": {
            "name": "距顶",
            "type": "string"
          },
          "bottom": {
            "name": "距底",
            "type": "string"
          },
          "side": {
            "name": "距两侧",
            "type": "string"
          },
          "width": {
            "name": "宽度",
            "type": "string"
          }
        },
        "default": {
          "top": "40",
          "bottom": "20",
          "side": "20",
          "width": "40%"
        }
      },
      "xAxis": {
        "name": "xAxis",
        "type": "group",
        "children": {
          "boundaryGap": {
            "name": "boundaryGap",
            "type": "boolean"
          },
          "labelColor": {
            "name": "标签颜色",
            "type": "string"
          },
          "labelSize": {
            "name": "标签大小",
            "type": "number"
          },
          "splitNumber": {
            "name": "分割段数",
            "type": "number"
          },
          "splitLineType": {
            "name": "分割线类型",
            "type": "string"
          },
          "splitLineColor": {
            "name": "分割线颜色",
            "type": "string"
          }
        },
        "default": {
          "boundaryGap": false,
          "labelColor": "#999",
          "labelSize": 12,
          "labelInterval": 0.5,
          "splitNumber": 5,
          "splitLineType": "5-5",
          "splitLineColor": "rgba(58, 58, 58, 0.3)"
        }
      },
      "yAxis": {
        "name": "yAxis",
        "type": "group",
        "children": {
          "inverse": {
            "name": "是否反转",
            "type": "boolean"
          },
          "axisLineColor": {
            "name": "轴线颜色",
            "type": "string"
          },
          "axisLabelSize": {
            "name": "标签大小",
            "type": "number"
          },
          "axisLabelWidth": {
            "name": "标签宽度",
            "type": "number"
          },
          "axisLabelColor": {
            "name": "标签颜色",
            "type": "string"
          },
          "axisLabelMargin": {
            "name": "标签margin",
            "type": "number"
          },
          "shadowColor": {
            "name": "shadow颜色",
            "type": "string"
          },
          "interval": {
            "name": "标签显示间隔",
            "type": "number"
          }
        },
        "default": {
          "inverse": true,
          "axisLineColor": "#999",
          "axisLabelSize": 12,
          "axisLabelWidth": 50,
          "axisLabelColor": "#666",
          "axisLabelMargin": 35,
          "shadowColor": "rgba(56, 156, 255, 0.1)=rgba(255, 211, 0, 0.1)",
          "interval": 0
        }
      },
      "series": {
        "name": "series",
        "type": "group",
        "children": {
          "borderRadius": {
            "name": "柱子圆角",
            "type": "number"
          },
          "barWidth": {
            "name": "柱子宽度",
            "type": "number"
          }
        },
        "default": {
          "borderRadius": 8,
          "barWidth": 12
        }
      }
