          "lineColor": {
            "name": "轴线颜色",
            "type": "string"
          },
          "tickShow": {
            "name": "刻度是否显示",
            "type": "boolean"
          },
          "name": {
            "name": "坐标轴名称",
            "type": "string"
          },
          "nameLocation": {
            "name": "名称显示位置",
            "type": "string"
          },
          "nameGap": {
            "name": "名称和轴线的距离",
            "type": "number"
          },
          "nameSize": {
            "name": "名称字体大小",
            "type": "number"
          },
          "labelWidth": {
            "name": "名称宽度",
            "type": "number"
          },
          "labelTextOverflow": {
            "name": "名称溢出设置",
            "type": "string"
          },
          "labelEllipsis": {
            "name": "名称溢出显示文本",
            "type": "string"
          }
        },
        "default": {
          "lineShow": true,
          "lineWidth": 1,
          "lineColor": "#c2c2c2",
          "tickShow": true,
          "name": "",
          "nameLocation": "middle",
          "nameGap": 15,
          "nameSize": 12,
          "labelWidth": "60",
          "labelTextOverflow": "break",
          "labelEllipsis": "..."
        }
      },
      "yAxis": {
        "name": "y轴",
        "type": "group",
        "children": {
          "lineShow": {
            "name": "轴线是否显示",
            "type": "boolean"
          },
          "lineWidth": {
            "name": "轴线宽度",
            "type": "number"
          },
          "lineColor": {
            "name": "轴线颜色",
            "type": "string"
          },
          "tickShow": {
            "name": "刻度是否显示",
            "type": "boolean"
          },
          "splitLineShow": {
            "name": "分割线是否显示",
            "type": "boolean"
          },
          "splitLineWidth": {
            "name": "轴线宽度",
            "type": "number"
          },
          "splitLineColor": {
            "name": "轴线颜色",
            "type": "string"
          },
          "splitLineType": {
            "name": "分割线类型",
            "type": "string"
          },
          "name": {
            "name": "坐标轴名称",
            "type": "string"
          },
          "nameLocation": {
            "name": "名称显示位置",
            "type": "string"
          },
          "nameGap": {
            "name": "名称和轴线的距离",
            "type": "number"
          },
          "nameSize": {
            "name": "名称字体大小",
            "type": "number"
          }
        },
        "default": {
          "lineShow": false,
          "lineWidth": 1,
          "lineColor": "#c2c2c2",
          "tickShow": false,
          "splitLineShow": true,
          "splitLineWidth": 1,
          "splitLineColor": "#c2c2c2",
          "splitLineType": "dashed",
          "name": " ",
          "nameLocation": "middle",
          "nameGap": 15,
          "nameSize": 12
        }
      }
    },
