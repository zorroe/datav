{
  "name": "@ecnu-pierose/bar",
  "version": "0.0.8",
  "dependencies": {
    "bcore": "0.0.18",
    "jquery": "2.1.4",
    "lodash": "4.6.1",
    "echarts": "5.3.1"
  },
  "datav": {
    "cn_name": "柱状图",
    "icon": "",
    "protocol": 2,
    "type": [
      "regular"
    ],
    "view": {
      "width": "400",
      "height": "200",
      "minWidth": "200",
      "minHeight": "100"
    },
    "apis": {
      "source": {
        "handler": "render",
        "description": "接口描述",
        "fields": {
          "value": {
            "description": "值说明"
          }
        }
      }
    },
    "config": {
      "x": {
        "name": "x",
        "type": "string",
        "default": "tjn"
      },
      "y": {
        "name": "y",
        "type": "string",
        "default": "sl"
      },
      "colorField": {
        "name": "颜色映射",
        "type": "string",
        "default": "jzqj"
      },
      "tooltip": {
        "name": "tooltip",
        "type": "group",
        "children": {
          "formatter": {
            "name": "formatter",
            "type": "string"
          },
          "bgColor": {
            "name": "背景颜色",
            "type": "string"
          },
          "padding": {
            "name": "padding",
            "type": "number"
          },
          "textColor": {
            "name": "字体颜色",
            "type": "string"
          },
          "textSize": {
            "name": "字体大小",
            "type": "number"
          },
          "valueFixed": {
            "name": "value精度",
            "type": "number"
          },
          "confine": {
            "name": "是否限制在图标内",
            "type": "boolean"
          },
          "order": {
            "name": "是否按照series顺序",
            "type": "boolean"
          }
        },
        "default": {
          "formatter": "",
          "bgColor": "#fff",
          "padding": 5,
          "textColor": "#3A3A3A",
          "textSize": 14,
          "valueFixed": -1,
          "confine": false,
          "order": true
        }
      },
      "axisPointer": {
        "name": "指针",
        "type": "group",
        "children": {
          "shadowColor": {
            "name": "阴影颜色",
            "type": "string"
          }
        },
        "default": {
          "shadowColor": "rgba(56, 156, 255, 0.12)"
        }
      },
      "grid": {
        "name": "grid",
        "type": "group",
        "children": {
          "left": {
            "name": "左",
            "type": "string"
          },
          "right": {
            "name": "右",
            "type": "string"
          },
          "top": {
            "name": "顶",
            "type": "string"
          },
          "bottom": {
            "name": "底",
            "type": "string"
          }
        },
        "default": {
          "left": "10%",
          "right": "10%",
          "top": "60",
          "bottom": "60"
        }
      },
      "legend": {
        "name": "图例配置",
        "type": "group",
        "children": {
          "legendIsShow": {
            "name": "图例是否显示",
            "type": "boolean"
          },
          "legendLeft": {
            "name": "图例距左距离",
            "type": "string"
          },
          "legendTop": {
            "name": "图例距顶距离",
            "type": "string"
          },
          "legendOrient": {
            "name": "图例方向",
            "type": "string"
          },
          "textColor": {
            "name": "图例颜色",
            "type": "string"
          },
          "textSize": {
            "name": "图例字大小",
            "type": "number"
          },
          "icon":{
            "name": "图例图标",
            "type": "string"
          },
          "itemWidth": {
            "name": "图形宽度",
            "type": "number"
          },
          "itemHeight": {
            "name": "图形高度",
            "type": "number"
          }
        },
        "default": {
          "legendIsShow": true,
          "legendLeft": "0%",
          "legendTop": "",
          "legendOrient": "horizontal",
          "textColor": "#3A3A3A",
          "textSize": 12,
          "icon":"rect",
          "itemWidth": 8,
          "itemHeight": 8
        }
      },
      "barOption": {
        "name": "柱子配置",
        "type": "group",
        "children": {
          "isStack": {
            "name": "是否堆叠",
            "type": "boolean"
          },
          "isRadius": {
            "name": "是否圆角",
            "type": "boolean"
          },
          "radiusSize": {
            "name": "圆角大小",
            "type": "number"
          },
          "width": {
            "name": "柱子宽度",
            "type": "number"
          },
          "linear": {
            "name": "是否渐变",
            "type": "boolean"
          }
        },
        "default": {
          "isStack": true,
          "isRadius": "true",
          "radiusSize": 8,
          "width": 10,
          "linear": false
        }
      },
      "xAxis": {
        "name": "x轴",
        "type": "group",
