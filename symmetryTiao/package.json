{
  "name": "@ecnu-pierose/symmetryTiao",
  "version": "0.0.2",
  "dependencies": {
    "bcore": "0.0.18",
    "jquery": "2.1.4",
    "lodash": "4.6.1",
    "echarts": "5.3.1"
  },
  "datav": {
    "cn_name": "对称条形图",
    "icon": "",
    "protocol": 2,
    "type": [
      "regular"
    ],
    "view": {
      "width": "400",
      "height": "200",
      "minWidth": "200",
      "minHeight": "100"
    },
    "apis": {
      "source": {
        "handler": "render",
        "description": "接口描述",
        "fields": {
          "value": {
            "description": "值说明"
          }
        }
      }
    },
    "config": {
      "dimensions": {
        "name": "维度",
        "type": "string",
        "default": "xxxs-硕士研究生-博士研究生"
      },
      "color": {
        "name": "颜色",
        "type": "string",
        "default": "rgba(41, 122, 252, 0.6)-rgba(41, 122, 252, 1)=rgba(242, 147, 24, 1)-rgba(242, 147, 24, 0.6)=#07A872-#0DD491"
      },
      "shadowColor": {
        "name": "shadow颜色",
        "type": "string",
        "default": "rgba(56, 156, 255, 0.10)-rgba(255, 211, 0, 0.10)"
      },
      "grid": {
        "name": "grid",
        "type": "group",
        "children": {
          "left": {
            "name": "距左",
            "type": "number"
          },
          "right": {
            "name": "距右",
            "type": "number"
          },
          "top": {
            "name": "距顶",
            "type": "number"
          },
          "bottom": {
            "name": "距底",
            "type": "number"
          },
          "containLabel": {
            "name": "containLabel",
            "type": "boolean"
          }
        },
        "default": {
          "left": 0,
          "right": 20,
          "top": 40,
          "bottom": 20,
          "containLabel": true
        }
      },
      "tooltip": {
        "name": "tooltip",
        "type": "group",
        "children": {
          "isShow": {
            "name": "是否显示",
            "type": "boolean"
          },
          "confine": {
            "name": "是否限制在图表内",
            "type": "boolean"
          },
          "fixed": {
            "name": "显示精度",
            "type": "number"
          },
          "suffix": {
            "name": "后缀",
            "type": "string"
          }
        },
        "default": {
          "isShow": true,
          "confine": true,
          "fixed": 0,
          "suffix": ""
        }
      },
      "legend": {
        "name": "图例配置",
        "type": "group",
        "children": {
          "isShow": {
            "name": "图例是否显示",
            "type": "boolean"
          },
          "left": {
            "name": "图例距左距离",
            "type": "string"
          },
          "top": {
            "name": "图例距顶距离",
            "type": "string"
          },
          "textColor": {
            "name": "图例文字颜色",
            "type": "string"
          },
          "textSize": {
            "name": "图例字大小",
            "type": "number"
          },
          "itemWidth": {
            "name": "图形宽度",
            "type": "number"
          },
          "itemHeight": {
            "name": "图形高度",
            "type": "number"
          },
          "itemGap": {
            "name": "图例间隔",
            "type": "number"
          }
        },
        "default": {
          "isShow": true,
          "left": "center",
          "top": "10",
          "textColor": "#3A3A3A",
          "textSize": 12,
          "itemWidth": 10,
          "itemHeight": 10,
          "itemGap": 10
        }
      },
      "xAxis": {
        "name": "xAxis",
        "type": "group",
        "children": {
          "inverse": {
            "name": "是否反转",
            "type": "boolean"
          },
          "min": {
            "name": "最小值",
            "type": "number"
          },
          "max": {
            "name": "最大值",
            "type": "number"
          },
          "labelColor": {
            "name": "标签颜色",
            "type": "string"
          },
          "labelSize": {
            "name": "标签大小",
            "type": "number"
          },
          "splitLineColor": {
            "name": "分割线颜色",
            "type": "string"
          },
          "splitLineType": {
            "name": "分割线类型",
            "type": "string"
          }
        },
        "default": {
          "inverse": true,
          "min": -4000,
          "max": 4000,
          "labelColor": "rgba(58, 58, 58, 0.50)",
          "labelSize": 12,
          "splitLineColor": "rgba(58, 58, 58, 0.30)",
          "splitLineType": "5-5"
        }
      },
      "yAxis": {
        "name": "yAxis",
        "type": "group",
        "children": {
          "inverse": {
            "name": "是否反转",
            "type": "boolean"
          },
          "labelColor": {
            "name": "label颜色",
            "type": "string"
          },
          "labelFontSize": {
            "name": "label字体大小",
            "type": "number"
          },
          "labelFontWeight": {
            "name": "label字重",
            "type": "number"
          },
          "lineColor": {
            "name": "轴线颜色",
            "type": "string"
          }
        },
        "default": {
          "inverse": true,
          "labelColor": "rgba(58, 58, 58, 0.50)",
          "labelFontSize": 12,
          "labelFontWeight": 400,
          "lineColor": "rgba(0,0,0,0)"
        }
      },
      "series": {
        "name": "series",
        "type": "group",
        "children": {
          "barRadius": {
            "name": "圆角大小",
            "type": "number"
          },
          "barWidth": {
            "name": "柱子宽度",
            "type": "number"
          }
        },
        "default": {
          "barRadius": 8,
          "barWidth": 12
        }
      }
    },
    "api_data": {
      "source": [
